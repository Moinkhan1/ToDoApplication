<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework"
      type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"
      requirePermission="false"/>
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8.1"/>
    </startup>
  
  
  <connectionStrings>
    <add name="TodoDbContext" connectionString="Server=localhost;Database=TodoDB;User Id=postgres;Password=admin;" providerName="Npgsql"/>
  </connectionStrings>
  <entityFramework>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer"/>
    </providers>
  </entityFramework>

  <system.serviceModel>
    <client>
      <endpoint address="http://localhost:57176/TodoService.svc" binding="basicHttpBinding" contract="ToDoApp.Shared.Services.IToDoService"/>
    </client>
  </system.serviceModel>
  
</configuration>
